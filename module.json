		}]
	},
    "url": "https://github.com/thejoester/preload-tracker",
    "manifest": "https://github.com/thejoester/preload-tracker/releases/latest/download/module.json",
    "download": "https://github.com/thejoester/preload-tracker/releases/download/1.0.1/preload-tracker.zip",
    "readme": "https://github.com/thejoester/preload-tracker#readme",
    "changelog": "https://github.com/thejoester/preload-tracker/releases",
    "bugs": "https://github.com/thejoester/preload-tracker/issues",
    "flags": {
        "canUpload": true
    }
}
