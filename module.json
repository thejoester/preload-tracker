{
    "id": "preload-tracker",
    "title": "Preload Tracker",
    "version": "1.0.1",
    "compatibility": {
        "minimum": "12",
        "verified": "13"
    },
    "description": "<p>When preloading a scene, show a window with status of all users and mark as they finish preloading.</p>",
    "authors": [
        {
            "name": "TheJoester",
            "url": "https://github.com/thejoester/",
            "discord": "thejoester",
            "flags": {}
        }
    ],
    "esmodules": [
        "scripts/tracker.js",
        "scripts/localization.js"
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "lang/en.json"
        },
        {
            "lang": "es",
            "name": "Spanish",
            "path": "lang/es.json"
        },
        {
            "lang": "pl",
            "name": "Polish",
            "path": "lang/pl.json"
        },
        {
            "lang": "fr",
            "name": "French",
            "path": "lang/fr.json"
        }
    ],
    "socket": true,
